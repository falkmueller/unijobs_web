{
  "name": "foo/bar",
  "require":
  {
	"twitter/bootstrap": "*",
     	"jquery/jquery": "*",
        "angularjs/angular": "*",
        "angularjs/angular-route": "*"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "jquery/jquery",
        "version": "1.11.3",
        "type": "jquery/jquery",
        "dist": {
          "url": "http://code.jquery.com/jquery-1.11.3.min.js",
          "type": "file"
        }
      }
    },
{
      "type": "package",
      "package": {
        "name": "twitter/bootstrap",
        "version": "3.3.4",
        "type": "twitter/bootstrap",
        "dist": {
          "url": "https://github.com/twbs/bootstrap/releases/download/v3.3.4/bootstrap-3.3.4-dist.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "angularjs/angular",
        "version": "1.3.15",
        "type": "angularjs/angular",
        "dist": {
          "url": "https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "angularjs/angular-route",
        "version": "1.3.15",
        "type": "angularjs/angular-route",
        "dist": {
          "url": "http://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular-route.js",
          "type": "file"
        }
      }
    }
  ],
"scripts": {
        "post-install-cmd": [
            "php install.php"
        ],
        "post-update-cmd": [
            "php install.php"
        ]
    }
}
